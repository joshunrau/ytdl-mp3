{
  "name": "ytdl-mp3",
  "version": "2.0.3",
  "description": "A NodeJS package and command-line tool for downloading music from YouTube, including automatic retrieval of ID3 tags and album art via iTunes.",
  "type": "commonjs",
  "bin": {
    "ytdl-mp3": "./bin/ytdl-mp3"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "format": "prettier --write src/**",
    "lint": "eslint --fix --ext .ts src/",
    "preversion": "npm run lint && npm run format && npm test",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "version": "npm run build",
    "ytdl-mp3": "ts-node ./bin/ytdl-mp3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshunrau/ytdl-mp3.git"
  },
  "author": "Joshua Unrau",
  "license": "MIT",
  "keywords": [
    "youtube",
    "download",
    "mp3",
    "music"
  ],
  "bugs": {
    "url": "https://github.com/joshunrau/ytdl-mp3/issues"
  },
  "homepage": "https://github.com/joshunrau/ytdl-mp3#readme",
  "devDependencies": {
    "@types/ffmpeg-static": "^3.0.1",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.0",
    "prettier": "2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "commander": "^10.0.0",
    "ffmpeg-static": "^5.1.0",
    "node-id3": "^0.2.5",
    "ytdl-core": "^4.11.2"
  },
  "files": [
    "bin",
    "build",
    "LICENSE",
    "README.md",
    "package.json"
  ]
}
