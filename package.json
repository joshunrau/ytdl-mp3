{
  "name": "ytdl-mp3",
  "version": "3.4.1",
  "description": "A ytdl-core wrapper that includes automatic retrieval of ID3 tags and album art via the iTunes public API",
  "keywords": [
    "youtube",
    "download",
    "mp3",
    "music"
  ],
  "homepage": "https://github.com/joshunrau/ytdl-mp3#readme",
  "bugs": {
    "url": "https://github.com/joshunrau/ytdl-mp3/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshunrau/ytdl-mp3.git"
  },
  "license": "MIT",
  "author": "Joshua Unrau",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ytdl-mp3": "./bin/ytdl-mp3.cjs"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write src",
    "lint": "eslint --fix src",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "commander": "^12.0.0",
    "ffmpeg-static": "^5.2.0",
    "node-id3": "^0.2.6",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "^3.0.3",
    "@types/node": "^20.12.2",
    "esbuild": "^0.20.2",
    "husky": "^9.0.11",
    "tsup": "^8.0.2",
    "typescript": "~5.4.3"
  }
}
